<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

    <!-- Servlets -->
    <servlet>
        <servlet-name>AppService</servlet-name>
        <servlet-class>com.sample.server.AppServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AppService</servlet-name>
        <url-pattern>/Sample/MyService</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AppService2</servlet-name>
        <servlet-class>com.sample.server.AppServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AppService2</servlet-name>
        <url-pattern>/Sample2/MyService</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AppService22</servlet-name>
        <servlet-class>com.sample.server.AppServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AppService22</servlet-name>
        <url-pattern>/Sample22/MyService</url-pattern>
    </servlet-mapping>
</web-app>
